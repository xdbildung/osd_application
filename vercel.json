{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/download-template",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/api/index.js"
    },
    {
      "src": "/setup",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
} 